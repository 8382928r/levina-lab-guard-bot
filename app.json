{
  "name": "Groups Guard Bot",
  "logo": "https://telegra.ph/file/8a6b57f97ca9db072d2e4.jpg",
  "description": "Groups Guard is a most complete Telegram bot for managing groups chat.",
  "keywords": [
    "telegram",
    "pyrogram",
    "python",
    "sql"
  ],
  "stack": "heroku-20",
  "repository": "https://github.com/levina-lab/guard-bot",
  "env": {
    "API_ID": {
      "description": "Your api_id from my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Your api_hash from my.telegram.org",
      "required": true
    },
    "TOKEN": {
      "description": "The bot_token from @BotFather",
      "required": true
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "10"
      }
    }
  ]
}
